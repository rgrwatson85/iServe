<%= form_for @user_table, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :user_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :user_id, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :table_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :table_id, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :assign_date, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :assign_date, :class => 'text_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                users_tables_path, :class => 'btn' %>
  </div>
<% end %>
