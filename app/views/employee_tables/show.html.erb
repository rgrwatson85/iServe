<%- model_class = EmployeeTable -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:employee_id) %>:</strong></dt>
  <dd><%= @employee_table.employee_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:table_id) %>:</strong></dt>
  <dd><%= @employee_table.table_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:assign_date) %>:</strong></dt>
  <dd><%= @employee_table.assign_date %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              employee_tables_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_employee_table_path(@employee_table), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              employee_table_path(@employee_table),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
